   "source": [
    "import math \n",
    "paso=0.1\n",
    "x1=0\n",
    "x2=paso\n",
    "suma=0\n",
    "while x2 <= 2:\n",
    "    fx2=  math.exp(-x2 * x2)\n",
    "    tria= (math.exp(-x1 * x1)- fx2)*paso/2\n",
    "    suma= suma + tria + fx2 * paso\n",
    "    x1= x2\n",
    "    x2 = x2 + paso\n",
    "print(suma)\n",
    "\n"
   ]
