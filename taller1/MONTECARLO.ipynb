    "import random\n",
    "import math\n",
    "n=0\n",
    "suma =0\n",
    "while n < 1000000:\n",
    "    x=2*random.random()\n",
    "    y= 2*math.exp(-x**2)\n",
    "    suma= suma + y\n",
    "    n=n+1\n",
    "print (suma/n)"
